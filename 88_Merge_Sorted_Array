class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        if(nums1.size() == 0 ){
            nums1 = nums2;
        } 
        else {
            std::copy(nums2.begin(), nums2.end(), nums1.begin()+m);
            std::sort(nums1.begin(), nums1.end());
        }
    }
};
